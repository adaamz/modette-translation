{
  "name": "modette/translation",
  "description": "Fast, simple and scalable translation solution",
  "keywords": [
    "modette",
    "nette",
    "neon",
    "latte",
    "translation"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/modette/translation",
  "authors": [
    {
      "name": "Marek Barto≈°",
      "homepage": "https://github.com/mabar"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "latte/latte": "^2.5",
    "nette/application": "^3.0",
    "nette/caching": "^3.0",
    "nette/di": "^3.0",
    "ninjify/nunjuck": "^0.2.0",
    "ninjify/qa": "^0.8.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "tracy/tracy": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Modette\\Translation\\": "src"
    },
    "files": [
      "src/shortcut.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Modette\\Translation\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
